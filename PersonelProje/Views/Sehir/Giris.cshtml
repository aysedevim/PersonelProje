@model PersonelProje.Data.Sehir


@{
    ViewData["Title"] = "Giris";
}

<h1>Giriş İşlemi</h1>
<div class="row">
    <div class="col-md-5">
       
        <form method="post">
            <div>
                <label>Id</label>
                <input class="form-control"  type="text" value="@Model.Id" name="Id" /> 
            </div>
            <div>
                <label>Ad</label>
                <input class="form-control" type="text" value="@Model.SehirAdi" name="SehirAdi" />
            </div>
            <input class="btn btn-primary" type="submit" value="Kaydet"/>
        </form>
    </div>
</div>

